{
  "package_id": "PKG-T3-ADMIN-001",
  "name": "Admin Agent",
  "version": "0.2.0",
  "tier": "T3",
  "capabilities": {
    "read": [
      "ledger/*.jsonl",
      "ledger/idx/*.json",
      "planes/*/ledger/*.jsonl",
      "planes/*/sessions/*/ledger/*.jsonl",
      "registries/*.csv",
      "registries/compiled/*.json",
      "installed/*/manifest.json",
      "installed/*/receipt.json",
      "config/*.json",
      "frameworks/*.md",
      "specs/*/manifest.yaml",
      "specs/*/*.md",
      "governed_prompts/*.md",
      "lib/*.py",
      "modules/**/*.py",
      "scripts/*.py"
    ],
    "execute": [
      "scripts/trace.py --explain *",
      "scripts/trace.py --installed",
      "scripts/trace.py --inventory",
      "scripts/trace.py --verify",
      "scripts/integrity_check.py --json"
    ],
    "write": [
      "planes/ho1/sessions/<session_id>/ledger/exec.jsonl",
      "planes/ho1/sessions/<session_id>/ledger/evidence.jsonl"
    ],
    "forbidden": [
      "scripts/package_install.py",
      "scripts/package_pack.py",
      "scripts/package_uninstall.py",
      "scripts/wo_approve.py",
      "scripts/kernel_install.py",
      "scripts/kernel_build.py",
      "scripts/apply_work_order.py"
    ],
    "llm_assisted": {
      "validate": true,
      "summarize": true,
      "explain": false,
      "general": true
    }
  },
  "metadata": {
    "description": "Admin agent for explaining and validating Control Plane artifacts",
    "author": "Control Plane",
    "created": "2026-02-03",
    "updated": "2026-02-04"
  }
}
